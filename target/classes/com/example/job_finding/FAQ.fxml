<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="1100.0" style="-fx-background-color: #000033;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.job_finding.FAQController">
   <children>
      <Label fx:id="FAQ_answer" alignment="TOP_LEFT" layoutX="3.0" layoutY="332.0" prefHeight="309.0" prefWidth="1101.0" text="Answer:" textFill="WHITE">
         <font>
            <Font name="Cambria" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="searchField" layoutX="424.0" layoutY="111.0" promptText="Type a Question:">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Button fx:id="FAQ_submit_button" layoutX="700.0" layoutY="114.0" mnemonicParsing="false" style="-fx-background-color: black; -fx-border-color: white; -fx-border-width: 2;" text="Go" textFill="#f4f4f4">
         <font>
            <Font name="Cambria Bold Italic" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
